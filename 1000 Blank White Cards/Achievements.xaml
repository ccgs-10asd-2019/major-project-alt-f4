        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1000_Blank_White_Cards"
        mc:Ignorable="d"
        Title="Achievements" Height="420" Width="700">
    <Viewbox StretchDirection="Both" Stretch="Uniform">
        <Grid Height="420" Width="700">
            <Image x:Name="Background" Source="otherImages/CrAb RaVe.jpg" Stretch="UniformToFill" Margin="0,0,0,0" Height="420"/>
            <Button x:Name="backButton" Content="Back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="ClimbLadder"/>
            <TextBlock HorizontalAlignment="Left" Margin="50,105,0,0" TextWrapping="Wrap" Text="This is currently not a working feature." VerticalAlignment="Top" Height="210" Width="600" FontSize="60" FontStyle="Italic"/>

        </Grid>
    </Viewbox>
