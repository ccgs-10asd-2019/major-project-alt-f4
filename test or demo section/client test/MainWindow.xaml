<Window x:Class="client_test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client_test"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Send" Content="Send" HorizontalAlignment="Left" Margin="286,143,0,0" VerticalAlignment="Top" Width="75" Click="send"/>
        <TextBox x:Name="textToSend" HorizontalAlignment="Left" Height="23" Margin="286,115,0,0" TextWrapping="Wrap" Text="text to send" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="reply" HorizontalAlignment="Left" Margin="286,168,0,0" TextWrapping="Wrap" Text="Reply" VerticalAlignment="Top"/>
        <Button x:Name="connect" Content="Connect" HorizontalAlignment="Left" Margin="150,163,0,0" VerticalAlignment="Top" Width="75" Click="Connect"/>
        <Button x:Name="disconnect" Content="Disconnect" HorizontalAlignment="Left" Margin="471,183,0,0" VerticalAlignment="Top" Width="75" Click="Disconnect"/>
        <Button x:Name="isaac" Content="Go to Isaac's design" HorizontalAlignment="Left" Margin="337,272,0,0" VerticalAlignment="Top" Width="75" Click="lmao"/>
    </Grid>
</Window>
